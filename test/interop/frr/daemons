# FRR daemons configuration for BFD interoperability testing.
#
# Only zebra and bfdd are enabled. No routing daemons are needed
# since we are testing BFD standalone (not integrated with BGP/OSPF).

zebra=yes
bgpd=no
ospfd=no
ospf6d=no
ripd=no
ripngd=no
isisd=no
pimd=no
ldpd=no
nhrpd=no
eigrpd=no
babeld=no
sharpd=no
pbrd=no
staticd=no
bfdd=yes
fabricd=no

vtysh_enable=yes

zebra_options="  -A 127.0.0.1 -s 90000000"
bfdd_options="   -A 127.0.0.1"
